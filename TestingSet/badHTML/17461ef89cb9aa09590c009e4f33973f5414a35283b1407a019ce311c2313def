                        <object width="220" height="120" classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,40,0"><param name="wmode" value="transparent" /><param name="base" value="/sabor/" /><param name="name" value="bannersaloneventos.swf" /><param name="movie" value="/index.php/img/images/lofthumbs/120x60/images/stories/plugins/system/scjquery/modules/mod_ice_accordion/js/images/stories/modules/mod_socialmedialinks/modules/mod_socialmedialinks/icons/default/size3/modules/mod_ice_slideshow/js/images/stories/images/stories/bannersaloneventos.swf" /><embed width="220" height="120" type="application/x-shockwave-flash" src="/index.php/img/images/lofthumbs/120x60/images/stories/plugins/system/scjquery/modules/mod_ice_accordion/js/images/stories/modules/mod_socialmedialinks/modules/mod_socialmedialinks/icons/default/size3/modules/mod_ice_slideshow/js/images/stories/images/stories/bannersaloneventos.swf" wmode="transparent" base="/sabor/" name="bannersaloneventos.swf"></embed></object>                        </div>
           
                                                            	                              
