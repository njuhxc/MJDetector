        var SymRealOnLoad;    var SymRealOnUnload;    var SymRealWinOpen;    function SymOnUnload() {      window.open = SymWinOpen;      if(SymRealOnUnload != null)        SymRealOnUnload();    }    function SymOnLoad() {      if(SymRealOnLoad != null)        SymRealOnLoad();      window.open = SymRealWinOpen;      SymRealOnUnload = window.onUnload;      window.onUnload = SymOnUnload;    }    SymRealOnLoad = window.onLoad;    window.onLoad = SymOnLoad;    //   