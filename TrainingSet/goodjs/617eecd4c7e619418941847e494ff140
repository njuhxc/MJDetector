        function recent_tweets(data) {            for (i=0; i<5; i++) {                document.getElementById("tweets").innerHTML =                    document.getElementById("tweets").innerHTML +                    '<a href="http://twitter.com/UltraLinx/status/' +                    (data[i].id_str ? data[i].id_str : data[i].id) +                    '"><div class="tweet">' + data[i].text +                    '</div></a>';            }            document.getElementById("twitter").style.display = 'block';        }    