function closeplayer(){ var container = document.getElementById('mydiv'); container.innerHTML = "start-add-div-contentend-add-div-content<img onClick='openplayer()' src='replay2.gif'>";}function openplayer(){   var fo = new SWFObject("a2dvideo.swf", "a2dvideo", "400", "500", "8", "transparent");		fo.addParam("wmode", "transparent");	fo.addParam("FlashVars", "datalink=data/captions.xml");		fo.write("mydiv");}