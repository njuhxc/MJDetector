jQuery(document).ready(function($){			jQuery(document).delegate(".pps-button-popup-5584, a[href=pps-button-popup-5584]", "click", function(e) {			e.preventDefault();						if(jQuery("#pps-slider-5584").length){				if(typeof jQuery.fn.popupslider == "function") {					jQuery("#pps-slider-5584").popupslider({						slideshow: false,						slideshowSpeed: 6000,						animationSpeed: 300,						controlNav: true,						directionNav: true,						pauseOnHover: true,						namespace: "pps-",						startAt: 0,						before: function(){							pauseVideosPopupPress(5584);						},						after: function(){							refreshTopPosition(5584);						},						selector: "#slides-5584 > li",					});					}				}			jQuery("#popuppress-5584").bPopup({				closeClass: "pps-close-link-5584",				easing: "swing",				modalClose: true,				modalColor: "#000000",				opacity: 0.7,				positionStyle: "absolute",				position: ["auto","auto"],				speed: 300,				transition: "fadeIn",				zIndex: 99999,				amsl : 0,				onOpen: function(){					contentFromIdPopupPress(5584,"");					updateViewsPopupPress(5584);					restoreVideosPopupPress(5584);					onOpenPopupPress(5584);					closeSettingsPopupPress(5584, "false");					//centerPopupPress(5584);				},				onClose: function(){					pauseVideosPopupPress(5584);					removeVideosPopupPress(5584);				},			});			});});