jQuery(function($){$(document).ready(function(){						$('.carousel').elegantcarousel({					delay:150,					fade:300,					slide:500,					effect:'fade',					  					orientation:'horizontal',					loop: true,					autoplay: false,					time: 4000			});									// horizontal center of the main  			function center_main() {				var window_height = $(window).height();				var main_height = parseInt($('#main').css('height'));				var main_height_margin = (window_height - main_height) / 2;				$('#main').css('top',Math.floor(main_height_margin));			}			center_main();			});	});