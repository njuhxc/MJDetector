            window.fbAsyncInit = function() {                if ( FB && FB.init ){            		FB.init( {appId: '230863646940515', status: true, cookie: true, xfbml: true });                }            };            (function() {                var e = document.createElement('script'); e.async = true;                e.src = "http://connect.facebook.net/en_US/all.js";                document.getElementById('fb-root').appendChild(e);            }());		