function submitForm(formID){  document.getElementById(formID).submit();}jQuery(document).ready(function() {   	jQuery('.flexform input[type="text"]').addClass("idleField");	jQuery('.flexform input[type="text"]').focus(function() {		jQuery(this).removeClass("idleField").addClass("focusField");        if (this.value == this.defaultValue){        	this.value = '';    	}        if(this.value != this.defaultValue){	    	this.select();        }    });    jQuery('.flexform input[type="text"]').blur(function() {    	jQuery(this).removeClass("focusField").addClass("idleField");        if (this.value == ''){        	this.value = (this.defaultValue ? this.defaultValue : '');    	}    });function flashing() {		jQuery('.flashing a').delay(2000).fadeTo(1000,0);		jQuery('.flashing a').fadeTo(1000,1, flashing);		   }flashing();});