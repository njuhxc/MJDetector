<script  language="Javascript">function mypop(url,title,w,h) {	if((title == "") || (title == "undefined"))	{		title = "mypop";	}	if((w == "") || (w == "undefined"))	{		w = 600;	}	if((h == "null") || (h == "undefined"))	{		h = 600;	}	var param = 'width=' + w + ',height=' + h ;	param += ',resizable=yes,scrollbars=no,top=0,left=0';	var hWnd = window.open(url,title,param);	// if (hWnd.focus != null) hWnd.focus();	hWnd.focus();}//