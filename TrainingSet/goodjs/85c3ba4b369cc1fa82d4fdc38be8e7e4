			var windowOpen;		jQuery(document).on( 'ready post-load', function(){			jQuery( 'a.share-twitter' ).on( 'click', function() {				if ( 'undefined' !== typeof windowOpen ){ // If there's another sharing window open, close it.					windowOpen.close();				}				windowOpen = window.open( jQuery(this).attr( 'href' ), 'wpcomtwitter', 'menubar=1,resizable=1,width=600,height=350' );				return false;			});		});		