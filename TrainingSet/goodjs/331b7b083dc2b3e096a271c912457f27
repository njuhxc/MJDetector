var crtg_nid = '823';var iol_adv_config = {'sito': 'virgilio'};$(document).ready(function (){ $.getJSON( "site_js/aphorisms366.js", function( data ) {    var now = new Date();    var start = new Date(now.getFullYear(), 0, 0);    var diff = now - start;    var oneDay = 1000 * 60 * 60 * 24;    var day = Math.floor(diff / oneDay);    if (data[day])    {      $("#aphorism").html(data[day][0]);      $("#author").html(data[day][1]);    }  });});