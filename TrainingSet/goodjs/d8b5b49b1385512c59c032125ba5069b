  function letter_or_dot(x){  if (parseInt(x) == x)  {    var range = 'azAZ..';    for (var i = 0; i < range.length; i += 2)         if (range.charCodeAt(i) <= x && x <= range.charCodeAt(i+1))           return true;  }  return false;}function do_text_select(o, tmplen){  if ("setSelectionRange" in o)  {    o.setSelectionRange(tmplen, o.value.length);  }  else if ("createTextRange" in o)  {    // IE 8-    var tr = o.createTextRange();    tr.moveStart("character", tmplen);    tr.moveEnd("character", o.value.len);    tr.select();  }}function Autocomplete(o, e, alist){	if (!o.value.match(/@/)) return true;	var key;	if (("key" in e) && "string" == typeof(e.key) && e.key.match(/^[A-Za-z.]$/))  {    key = e.key;	}  else if (("charCode" in e) && letter_or_dot(e.charCode))  {    key = String.fromCharCode(e.charCode);	}  else if (("keyCode" in e)  && letter_or_dot(e.keyCode))  {    key = String.fromCharCode(e.keyCode);	}  else  {    return true;	}	var tmp = o.value;	if ("selectionStart" in o && 0 <= o.selectionStart && o.selectionStart < o.selectionEnd)  {    tmp = o.value.substring(0, o.selectionStart);	}  else if ("selection" in document && "type" in document.selection && document.selection.type == "Text")  {	    tmp = o.value.substring(0, o.value.length - document.selection.createRange().text.length);	}    	tmp = tmp + key;	var seg = tmp.split('@', 2);	for (var i = 0; i < alist.length; ++i)  {    if (alist[i].indexOf(seg[1].toLowerCase()) == 0)    {	  	o.value = seg[0] + "@" + alist[i];	  	do_text_select(o, tmp.length);	  	return false;	  }	}	return true;}    var arrValues = ['virgilio.it'];//var box_err_mess = document.getElementById('box_err_mess');function checkparams( login_denomination ) {  if ( login_denomination == undefined )  {     login_denomination = 'tua email';     // login_denomination='Libero ID';     password_denomination = 'tua password';  }  else  {     login_denomination = 'email che vuoi  creare';     password_denomination = 'password';  }     if ( ! document.autenticazione.LOGINID.value && ! document.autenticazione.PASSWORD.value )  {     box_err_mess.innerHTML = 'Inserisci la ' + login_denomination + ' e la ' + password_denomination;     document.autenticazione.LOGINID.focus();     return false;  }  if ( document.autenticazione.LOGINID.value == '' )  {     box_err_mess.innerHTML = 'Inserisci la ' + login_denomination;     document.autenticazione.LOGINID.focus();     // document.autenticazione.LOGINID.style.borderColor = 'red';     return false;  }  if ( document.autenticazione.PASSWORD.value == '' )  {     box_err_mess.innerHTML = 'Inserisci la ' + password_denomination;     document.autenticazione.PASSWORD.focus();     // document.autenticazione.PASSWORD.style.borderColor = 'red';     return false;  }  return true;}  try { document.autenticazione.LOGINID.focus() }  catch ( err ) {}