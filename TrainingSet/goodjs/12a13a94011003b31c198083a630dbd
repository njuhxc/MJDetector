function mulvote(){	var obj = document.all.VoteList_More;	var litem ="";	for (i = 0 ; i < obj.length; i ++)	{		if (obj[i].checked)		{			litem = litem + obj[i].value + ",";		}	}	if(litem=="")	{		alert("???????:????????????????,�ݧ�!");		return(false);	}	else	{		litem = litem.substring(0,litem.length-1);		var url = "/Modules/Other/Vote/?VoteID=" + document.all.VoteID.value +"&ListItem=" + litem + "&VoteType=1&ViewType=0";		var popup = window.open(url,'mulvote','top=50,left=50,toolbar=0,scrollbars=yes,resizable=no,width=620,height=400');		return;	}}//