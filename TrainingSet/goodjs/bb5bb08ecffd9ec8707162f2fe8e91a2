var jQuery = jQuery.noConflict();jQuery(function() {    jQuery('.qim_footguide_foot_close').bind('click',    function() {        jQuery('.qim_footguide_foot').css({            bottom: '-120px'        });        jQuery(this).attr('id', '1');        jQuery('.fixed_left').css({            bottom: '0'        });    })    jQuery('.fixed_left').bind('click',    function() {        jQuery('.qim_footguide_foot').css({            bottom: '0px'        });        jQuery('.qim_footguide_foot_close').attr('id', '0');        jQuery('.fixed_left').css({            bottom: '-160px'        });    })    jQuery(window).scroll(function() {        if (jQuery('.qim_footguide_foot_close').attr('id') != '1') {            if (jQuery(window).scrollTop() > 100) {                jQuery('.qim_footguide_foot').css({                    bottom: '0px'                });            } else {                jQuery('.qim_footguide_foot').css({                    bottom: '-120px'                });            }        }    })})