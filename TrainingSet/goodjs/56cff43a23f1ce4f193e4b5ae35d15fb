			jQuery(document).on('ready', function(){				jQuery('body').on('click', 'a.share-pinterest', function(e){					e.preventDefault();					// Load Pinterest Bookmarklet code					var s = document.createElement("script");					s.type = "text/javascript";					s.src = window.location.protocol + "//assets.pinterest.com/js/pinmarklet.js?r=" + ( Math.random() * 99999999 );					var x = document.getElementsByTagName("script")[0];					x.parentNode.insertBefore(s, x);					// Trigger Stats					var s = document.createElement("script");					s.type = "text/javascript";					s.src = this + ( this.toString().indexOf( '?' ) ? '&' : '?' ) + 'js_only=1';					var x = document.getElementsByTagName("script")[0];					x.parentNode.insertBefore(s, x);				});			});			