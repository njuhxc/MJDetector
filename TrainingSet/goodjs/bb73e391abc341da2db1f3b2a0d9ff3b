<script  type="text/javascript" language="javascript1.2">	jQuery(document).ready(function(){		jQuery('a.mrtUnsubscribeLink').click(function(e){					e.preventDefault();							var ajaxurl	= "http://lindygolden.com/wp-admin/admin-ajax.php";			var data 	= {							action		: 'mrt-unsubscribed-form',						  };									  			jQuery.post(ajaxurl, data, function(response) {				jQuery.nmData(response,{					sizes	: {								initW	: 500,								initH	: 400,								w		: 500,								h		: 400,								minW	: 500,								minH	: 400						}				})			});									return false;		});	});	