	function bbbet(betid, next, lng){		$('#bbbetday').fadeOut();		$.post("/ajax/bbbet.php", { betid: betid, next: next, lng: lng }, function(data){				$('#bbbetday').hide();				$('#bbbetday').html(data);		$('#bbbetday').fadeIn();			});		}function nnnws(nwsid, next, lng){		$('#nnnwshour').fadeOut();		$('#bbp1').css('background-image', 'url(//cdn.livetvcdn.net/img/lbg5.gif)');			$.post("/ajax/nnnws.php", { nwsid: nwsid, next: next, lng: lng }, function(data){				$('#nnnwshour').hide();				$('#nnnwshour').html(data);		$('#nnnwshour').fadeIn();			});		}