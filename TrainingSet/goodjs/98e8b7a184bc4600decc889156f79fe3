    function get_q_param(separator) {        var last_segment = null;        var segment_str = window.location.pathname;        var segment_array = segment_str.split('/');        last_index = segment_array.length - 1;        if (segment_array[last_index]) {            last_segment = segment_array[last_index];        } else {            last_segment = segment_array[last_index - 1];        }        last_segment = last_segment.split(separator).join(' ');        return last_segment;    }    q_param = get_q_param('/');