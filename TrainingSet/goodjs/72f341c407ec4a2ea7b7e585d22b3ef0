jQuery(document).ready(function($){if($.cookie('popup_facebook_box') != 'yes'){	var d = new Date();	d.setTime(d.getTime() + (12*60*60*1000));	var expires = "expires="+ d.toUTCString();	document.cookie = "popup_facebook_box=yes;" + expires;	$('#fbox-background').delay(0).fadeIn('medium');	$('#fbox-button, #fbox-close').click(function(){	$('#fbox-background').stop().fadeOut('medium');});}});