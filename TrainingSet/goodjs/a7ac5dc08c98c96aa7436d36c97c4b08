<script  type="text/javascript" language="javascript1.2">				function delayer()		{ window.location = "http://www.lindygolden.com" }				jQuery(document).ready(function(){			jQuery('#mobile-register-widget-2 form').submit(function(){				var ajaxurl	= "http://lindygolden.com/wp-admin/admin-ajax.php";				var data	= jQuery(this).serialize();								jQuery.ajax({	            	url		: "http://lindygolden.com/wp-admin/admin-ajax.php",	                type	: 'POST',	                data	: data,					success	: function(results)					{						var string	= results.substring(0, results.length-2);						var success	= results.substring(results.length-1, results.length-2);						//jQuery('#mobile-register-widget-2 .message-wrapper').html(string).fadeIn('fast');						jQuery('#mobile-register-widget-2 .message-wrapper').html(string).fadeIn('fast').delay(10000).fadeOut('slow');																		if(success == 1) { setTimeout('delayer()', 5000) }											}				});				return false;			});		});		