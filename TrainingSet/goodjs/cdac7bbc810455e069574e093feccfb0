                $(document).ready(function(){            $('.js-download').bind('click', function(e){                if(!$('.js-download').hasClass('js-click-done')) {                    var eventSubstep = $(e.currentTarget).data('substep') ? '&substep=download_button_click_' + $(e.currentTarget).data('substep') : '';                    $('.js-download').addClass('js-click-done');                    var eventJsDwnldIframe = document.createElement('iframe');                    setTimeout(function () {                        eventJsDwnldIframe.src = "https://event.mackeeper.com/event.php?step=Download_Button_Click" + eventSubstep + "&affid=mzb_243.185396.1436166024.4.mzb&bundleid=29_21400&prodid=29&response=json";                        eventJsDwnldIframe.style.display = "none";                        document.body.appendChild(eventJsDwnldIframe);                    }, 850);                }            });                    });    