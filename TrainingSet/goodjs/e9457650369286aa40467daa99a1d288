        function loadComments(page) {                $('<div>').load(decode64('aHR0cDovL2VnbGVuY2VsaS5veXVudW95bmEuY29tL0F0ZXMtdmUtU3UtT25saW5lLTkyNTUuaHRtbD9hamF4PTEmY29tbWVudHM9MQ==')+'&s=' + page, function(r){                $('.game-comments-container').replaceWith(r);            });            return false;        }        function likeComment(commentId, like) {            $('<div>').load('/ajax/comment/like', {                action:'like',                commentid:commentId,                like:like == true ? '1' : '',            },function(r){                loadComments(1);            });        }    